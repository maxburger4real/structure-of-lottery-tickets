   "source": [
    "\n",
    "import networkx as nx\n",
    "G = nx.DiGraph()\n",
    "G.add_nodes_from([1,2,3,4])\n",
    "G.add_edge(1,2)\n",
    "G.add_edge(3,4)\n",
    "G.add_edge(5,4)\n",
    "\n",
    "G = G.to_undirected()\n",
    "\n",
    "[G.subgraph(c) for c in nx.connected_components(G)]"
   ]
